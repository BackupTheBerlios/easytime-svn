?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"config/hibernate-mapping-2.0.dtd">

<hibernate-mapping
<class name="src.server.resource.device.DeviceKind" table="DEVICE_KIND">
		<id name="id">
			<generator class="native"/>
        </id>
       	<property name="name" type="String"/>
       	<many-to-one name="kindId" class=src.server.resource.device.Device"/>
    </class>
</hibernate-mapping>
       	