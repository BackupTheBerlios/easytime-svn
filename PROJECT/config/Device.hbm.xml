?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"config/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="src.server.resource.device.Device" table="DEVICE">
		<id name="id">
			<generator class="native"/>
        </id>
       	<property name="typeID" type="int"/>
       	<set name="devicekinds">
       		<key column="kindId"/>
       		<one-to-many class=src.server.resource.device.DeviceKind"/>
       	</set>
    </class>
</hibernate-mapping>
       	